    "v": [
      {
        "t": "t",
        "v": "Flee from being slaves of the people of Babylonia!",
        "id": 151
      }
    ],
    "id": 150
  },
  {
    "t": "q",
    "v": [
      {
        "t": "t",
        "v": "Proclaim this message joyfully;",
        "id": 153
      }
    ],
    "id": 152
  },
