    "v": [
      {
        "t": "t",
        "v": "He disguises his face.",
        "id": 71
      }
    ],
    "id": 70
  },
  {
    "t": "q",
    "v": [
      {
        "t": "v",
        "n": 16,
        "v": "In the darkness wicked people dig into houses;",
